<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="OFC_TEMPLATEFILE">
	<typeAlias alias="TemplateFile" type="tecamo.module.office.domain.TemplateFile" />
	<resultMap id="OFC_TEMPLATEFILE_TemplateFileResult" class="TemplateFile">
		<result column="ID" property="id" nullValue="0" jdbcType="DECIMAL" />
		<result column="FILENAME" property="fileName" nullValue="" jdbcType="VARCHAR" />
		<result column="FILETYPE" property="fileType" nullValue="" jdbcType="VARCHAR" />
		<result column="FILESIZE" property="fileSize" nullValue="0" jdbcType="DECIMAL" />
		<result column="FILEDATE" property="fileDate" nullValue="" jdbcType="VARCHAR" />
		<result column="FILEBODY" property="fileBody" jdbcType="BOLB" />
		<result column="FILEPATH" property="filePath" nullValue="" jdbcType="VARCHAR" />
		<result column="USERNAME" property="userName" nullValue="" jdbcType="VARCHAR" />
		<result column="DESCRIPT" property="descript" nullValue="" jdbcType="VARCHAR" />
	</resultMap>
	<select id="OFC_TEMPLATEFILE_getById" resultMap="OFC_TEMPLATEFILE_TemplateFileResult" parameterClass="long">
		select ID,FILENAME,FILETYPE,FILESIZE,FILEDATE,FILEBODY,FILEPATH,USERNAME,DESCRIPT from OFC_TEMPLATEFILE
		where ID = #id:DECIMAL#
	</select>
	<delete id="OFC_TEMPLATEFILE_delById" parameterClass="long">
		delete from OFC_TEMPLATEFILE
		where ID = #id:DECIMAL#
	</delete>
	<insert id="OFC_TEMPLATEFILE_add" parameterClass="TemplateFile">
		insert into OFC_TEMPLATEFILE (ID,FILENAME,FILETYPE,FILESIZE,FILEDATE,FILEBODY,FILEPATH,USERNAME,DESCRIPT)
		values (#id#,#fileName#,#fileType#,#fileSize#,#fileDate#,#fileBody#,#filePath#,#userName#,#descript# )
	</insert>
	<update id="OFC_TEMPLATEFILE_update" parameterClass="TemplateFile">
			update OFC_TEMPLATEFILE set
			FILENAME=#fileName#,
			FILETYPE=#fileType#,
			FILESIZE=#fileSize#,
			FILEDATE=#fileDate#,
			FILEBODY=#fileBody#,
			FILEPATH=#filePath#,
			USERNAME=#userName#,
			DESCRIPT=#descript#
		where ID = #id:DECIMAL#
	</update>
</sqlMap>